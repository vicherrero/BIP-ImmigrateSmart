<script type="text/javascript">
function registerPreSubmitCheck()
    {
        
        var err=document.getElementById('error');
        var fErr=document.getElementById('formError');
        var name=document.getElementById('name').value;
        var uname=document.getElementById('username').value;
        var email=document.getElementById('email').value;
        var pwd=document.getElementById('password').value;
        if(email.indexOf('@')>-1 && email.indexOf('.')>-1 && email.indexOf('@')< email.lastIndexOf('.'))
        {
	        	
	        if(err.value==1 || uname.length===0 || name.length===0 || email.length===0 || pwd.length===0)
	        {
	            fErr.innerHTML="Please fill in all the required fields.";
	            return false;
	        }
	        else
	            {
	                document.getElementById('regForm').submit();
	            }
        }else{
        	 fErr.innerHTML="Email address is not correct!";
            return false;
        }
    }
    function matchPasswords()
    {
        //window.alert('heyy');
        var p1=document.getElementById('password');
        var p2=document.getElementById('cPassword');
        var err=document.getElementById('error');
        var pErr=document.getElementById('passError');
        if(p1.value==p2.value)
        {
            pErr.innerHTML='';
            err.value='0';
            return true;
            
        }else {
            //window.alert('Passwords do not match!'); 
            err.value='1';
            pErr.innerHTML='Passwords do not match!';
            return false;
            
        }    
    }    
</script>
